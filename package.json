{
  "name": "social-fabric",
  "version": "0.0.0",
  "repository": "https://github.com/efreethy/social-fabric-service",
  "description": "An attempt at a better social media platform",
  "author": "efreethy",
  "license": "ISC",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build": "npm run clean && npm run build:babel",
    "build:babel": "babel -d ./build ./src -s --copy-files",
    "postinstall": "npm run build",
    "start": "npm run build && npmDEBUG=express-sequelize node ./build/index.js",
    "start:dev": "npm run build && npmDEBUG=express-sequelize node ./build/index.js",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha build/test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha build/test/integration/*.test.js",
    "sort": "./node_modules/.bin/js-import-sort --path ./src/*"
  },
  "dependencies": {
    "bluebird": "^3.7.1",
    "body-parser": "^1.19.0",
    "continuation-local-storage": "^3.2.1",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-joi-validation": "^4.0.3",
    "express-jwt": "^5.3.1",
    "express-promise": "^0.4.0",
    "@hapi/joi": "^16.1.7",
    "js-import-sort": "^2.0.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "pg": "^7.14.0",
    "pug": "^2.0.4",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "sequelize-querystring": "^0.10.0",
    "serve-favicon": "~2.5.0",
    "short-unique-id": "^1.1.1",
    "sqlite3": "^4.1.0"
  },
  "devDependencies": {
    "@kant/babel-watch": "^3.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^6.0.3",
    "expect.js": "^0.3.1",
    "mocha": "^6.2.2",
    "supertest": "^4.0.2"
  }
}
